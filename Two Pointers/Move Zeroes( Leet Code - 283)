class Solution {
    public void moveZeroes(int[] nums) {
        int j=0;
        int count=0;
        for(int i:nums){
            if(i!=0){
                nums[j]=i;
                j++;
            }else{
                count++;
            }
        }
        for(int i=nums.length-count;i<nums.length;i++){
            nums[i]=0;
        }
    }
}
