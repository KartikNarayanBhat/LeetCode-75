class Solution {
    public double findMaxAverage(int[] nums, int k) {
        int sum=0;
        int max=0;
        for(int i=0;i<k;i++){
            sum+=nums[i];
        }
        max=sum;
        int i=0;
        int j=k;
        while(j<nums.length){
            sum-=nums[i];
            i++;
            sum+=nums[j];
            j++;
            max=Math.max(max,sum);
        }
    
        return (double)max/k;
    }
}
